<Window x:Class="SPNP_12.TaskWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP_12"
        mc:Ignorable="d"
        Title="TaskWindow" Height="450" Width="400">
    <StackPanel Orientation="Vertical">
        <Label>Многозадачность</Label>
        <Button x:Name="DemoBtn1" Content="Demo1" Click="DemoBtn1_Click"/>
        <Button x:Name="DemoBtn2" Content="Demo2" Click="DemoBtn2_Click"/>
        <TextBlock Text="" x:Name="LogTextBlock" Height="100"/>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="ParalelBtn" Width="45" Height="30" Click="ParalelBtn_Click">Paralel</Button>
            <ProgressBar x:Name="FirstProgressBar" Width="125" Margin="19,5,5,5"></ProgressBar>
            <ProgressBar x:Name="SecondProgressBar" Width="125" Margin="0,5,0,5"></ProgressBar>
            <Button x:Name="SubsequenceBtn" Width="45" Height="30" Margin="20 0 0 0" Click="SubsequenceBtn_Click">Subseq</Button>
        </StackPanel>
    </StackPanel>
</Window>
