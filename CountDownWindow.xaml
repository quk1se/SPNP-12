<Window x:Class="SPNP_12.CountDownWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP_12"
        mc:Ignorable="d"
        Title="CountDownWindow" Height="100" Width="300" Loaded="Window_Loaded">
    <StackPanel Orientation="Vertical">
        <Label Content="Close another window or this automatic close" HorizontalAlignment="Center"/>
        <ProgressBar x:Name="progressBar1" Height="25" Value="100"/>
    </StackPanel>
</Window>
