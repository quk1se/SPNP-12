<Window x:Class="SPNP_12.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP_12"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300">
    <StackPanel>
        <Label HorizontalAlignment="Center" FontWeight="Bold" FontSize="18">Системне програмування</Label>
        <Button x:Name="ThreadingBtn" Margin="20 7" Click="ThreadingBtn_Click">Многопоточность</Button>
        <Button x:Name="SynchroBtn" Margin="20 7" Click="SynchroBtn_Click">Синхронизация</Button>
        <Button x:Name="ManyBtn" Margin="20 7" Click="ManyBtn_Click">Многозадачность</Button>
        <Button x:Name="CancelBtn" Margin="20 7" Click="CancelBtn_Click">Отмена задач</Button>
    </StackPanel>
</Window>
