<Window x:Class="SPNP_12.DllWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP_12"
        mc:Ignorable="d"
        Title="DllWindow" Height="450" Width="300">
    <StackPanel>
        <Button x:Name="AlertButton"
        Content="Alert"
        Margin="10 7"
        Click="AlertButton_Click"/>
        <Button x:Name="ThreadBtn"
        Content="Thread"
        Margin="10 7"
        Click="ThreadBtn_Click"/>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="sound1" Content="Sound1" Margin="0 0 20 0"  Click="sound1_Click"/>
            <Button x:Name="sound2" Content="Sound2" Margin="20 0 5 0" Click="sound2_Click"/>
            <Button x:Name="sound3" Content="Sound3" Margin="20 0 20 0" Click="sound3_Click"/>
            <Button x:Name="sound4" Content="Sound4" Margin="20 0 0 0" Click="sound4_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
